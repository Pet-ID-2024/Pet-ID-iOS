def pods_networking
	pod 'Moya/Combine', '~> 15.0' #
end

def pods_log
	pod 'CocoaLumberjack/Swift', '~> 3.8' #
end


def pods_ui
	pod 'Toast-Swift', '~> 5.0' #
end

def pods_socialLoginSDK
  pod 'KakaoSDK'
end

def pods_AI
  pod 'TensorFlowLiteSwift'
end

platform :ios, '15.0'

target 'Pet-ID-iOS' do
  use_frameworks!
	pods_networking
	pods_log
	pods_ui
	pods_socialLoginSDK
	pods_AI
end
